    "import xml.etree.ElementTree as ET\n",
    "\n",
    "tree = ET.parse('../../data/trial/subtask1-homographic-test.xml')\n",
    "root = tree.getroot()\n",
    "\n",
    "for i, sent in enumerate(root):\n",
    "    text = [word.text for word in sent]\n",
    "    print(sent.attrib['id'], ' '.join(text))"
   ]
